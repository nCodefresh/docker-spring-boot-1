version: '3'
services:
  my-service:
    image: 'containers101/whomi:master'
    volumes:
      - my-service:/app
    ports:
      - '1337'
  new-terminal:
    build:
      context: ./
      dockerfile: Dockerfile
  terminal:
    build:
      context: ./
      dockerfile: Dockerfile
    ports:
      - '8000'
    volumes_from:
      - my-service
volumes:
  my-service:
    driver: local